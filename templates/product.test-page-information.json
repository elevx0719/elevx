        "custom_liquid_RPFdzh": {
          "type": "custom-liquid",
          "name": "t:names.custom_liquid",
          "settings": {
            "custom_liquid": "<style>\n.modern-prod-flexbox {\n  display: flex;\n  gap: 2rem;\n  background: #fff;\n  border-radius: 20px;\n  box-shadow: 0 4px 24px rgba(0,0,0,0.07);\n  padding: 2rem;\n  margin-bottom: 2.2rem;\n  align-items: flex-start;\n  max-width: 1000px;\n}\n@media (max-width: 900px) {\n  .modern-prod-flexbox { flex-direction: column; padding: 1rem; gap: 1.3rem; }\n}\n.prod-images-slider {\n  width: 350px;\n  min-width: 0;\n}\n@media (max-width: 600px) {\n  .prod-images-slider { width: 100%; max-width: 99vw; }\n}\n.flickity-slider {\n  border-radius: 14px;\n  overflow: hidden;\n}\n.carousel-cell img {\n  display: block;\n  width: 100%;\n  border-radius: 14px;\n  height: auto;\n  background: #f7f7f7;\n}\n.prod-info-box {\n  flex: 1;\n}\n.prod-info-box h1 {\n  font-size: 2.1rem;\n  font-weight: 700;\n  margin-bottom: 0.5rem;\n}\n.prod-info-box .price {\n  color: #18804b;\n  font-size: 1.4rem;\n  font-weight: 600;\n  margin-bottom: 1.2rem;\n}\n.prod-info-box .desc {\n  font-size: 1.08rem;\n  color: #3a3a3a;\n  margin-bottom: 1.3rem;\n  line-height: 1.6;\n}\n.prod-info-box .highlights {\n  display: flex;\n  gap: 1rem;\n  flex-wrap: wrap;\n  margin-bottom: 1.3rem;\n}\n.prod-info-box .highlight {\n  background: #f8f9fa;\n  padding: 0.7rem 1.1rem;\n  border-radius: 9px;\n  font-size: 0.99rem;\n  color: #2c2c2c;\n  flex: 1 0 38%;\n  min-width: 120px;\n  box-shadow: 0 2px 8px rgba(20,128,74,0.04);\n}\n.prod-info-box .buy-btn {\n  background: linear-gradient(90deg, #34e89e, #0f3443);\n  color: #fff;\n  font-size: 1.09rem;\n  font-weight: 600;\n  padding: 0.9rem 2.2rem;\n  border: none;\n  border-radius: 15px;\n  box-shadow: 0 4px 14px rgba(52,232,158,0.14);\n  cursor: pointer;\n  transition: transform 0.13s;\n  text-decoration: none;\n  display: inline-block;\n}\n.prod-info-box .buy-btn:hover {\n  transform: translateY(-2px) scale(1.03);\n}\n</style>\n\n<div class=\"modern-prod-flexbox\">\n  <!-- LEFT: Product Image Slider -->\n  <div class=\"prod-images-slider js-flickity\"\n    data-flickity-options='{ \"wrapAround\": true, \"autoPlay\": 3500, \"pageDots\": true, \"prevNextButtons\": false }'>\n    {% for media in product.media %}\n      {% if media.media_type == 'image' %}\n        <div class=\"carousel-cell\">\n          <img src=\"{{ media | img_url: '800x' }}\" alt=\"{{ media.alt | escape }}\">\n        </div>\n      {% endif %}\n    {% endfor %}\n  </div>\n  <!-- RIGHT: Product Info -->\n  <div class=\"prod-info-box\">\n    <h1>{{ product.title }}</h1>\n    <div class=\"price\">\n      {% if product.compare_at_price > product.price %}\n        <span style=\"color: #cb4154; text-decoration:line-through; font-size: 1.2rem;\">{{ product.compare_at_price | money }}</span>\n      {% endif %}\n      {{ product.price | money }}\n    </div>\n    <div class=\"desc\">{{ product.description | strip_html | truncatewords: 40 }}</div>\n    <div class=\"highlights\">\n      <div class=\"highlight\">\n        <strong>Type:</strong> {{ product.type }}\n      </div>\n      <div class=\"highlight\">\n        <strong>Vendor:</strong> {{ product.vendor }}\n      </div>\n      {% if product.selected_or_first_available_variant.sku != blank %}\n      <div class=\"highlight\">\n        <strong>SKU:</strong> {{ product.selected_or_first_available_variant.sku }}\n      </div>\n      {% endif %}\n      <div class=\"highlight\">\n        <strong>Availability:</strong>\n        {% if product.available %}<span style=\"color: #18b77b;\">In stock</span>{% else %}<span style=\"color: #cb4154;\">Sold out</span>{% endif %}\n      </div>\n    </div>\n    <a href=\"/cart/add?id={{ product.selected_or_first_available_variant.id }}\" class=\"buy-btn\">Add to Cart</a>\n  </div>\n</div>"
          },
          "blocks": {}
        },
